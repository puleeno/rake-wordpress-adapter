{
    "name": "puleeno/rake-wordpress-adapter",
    "description": "The WordPress adapter for Rake crawler/spider framework",
    "type": "library",
    "require": {
        "ramphor/rake": "^2",
        "ramphor/logger": "*",
        "rappasoft/laravel-helpers": "^2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Puleeno Nguyen",
            "email": "puleeno@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Puleeno\\Rake\\WordPress\\": "src/"
        },
        "files": [
            "./helpers.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "alleyinteractive/composer-wordpress-autoloader": true
        }
    }
}
